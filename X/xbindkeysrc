# swipe 4 left -> switch to previous workspace
"awesome-client awesome.emit_signal\(\'tag_switch\',-1\)"
	b:11

# same but with mouse
"awesome-client awesome.emit_signal\(\'tag_switch\',-1\)"
	b:8

# swipe 4 right -> switch to next workspace
"awesome-client awesome.emit_signal\(\'tag_switch\',1\)"
	b:10

# same but with mouse
"awesome-client awesome.emit_signal\(\'tag_switch\',1\)"
	b:9
